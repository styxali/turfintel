{
  "name": "ts-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "worker": "tsx src/workers/ingestion-worker.ts",
    "worker:dev": "tsx watch src/workers/ingestion-worker.ts",
    "dev:old": "ts-node server.ts",
    "dev:watch": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset",
    "db:seed": "ts-node prisma/seed.ts",
    "test": "echo 'Tests not yet implemented' && exit 0"
  },
  "dependencies": {
    "@langchain/community": "^0.0.20",
    "@langchain/core": "^0.1.63",
    "@langchain/openai": "^0.0.19",
    "@prisma/client": "6.19.1",
    "axios": "^1.6.0",
    "axios-cookiejar-support": "^6.0.5",
    "axios-retry": "^4.5.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "hnswlib-node": "^1.4.2",
    "langchain": "^0.1.0",
    "luxon": "^3.4.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tough-cookie": "^6.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/luxon": "^3.7.1",
    "@types/sqlite3": "^3.1.11",
    "prisma": "6.19.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
